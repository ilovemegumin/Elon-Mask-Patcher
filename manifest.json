{
  "manifest_version": 3,
  "name": "Elon Mask Patcher",
  "version": "1.0.0",
  "description": "XのDarkBlueThemeを復活させます",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://x.com/*", "https://twitter.com/*"],
      "css": ["style.css"],
      "run_at": "document_start"
    }
  ]
}
